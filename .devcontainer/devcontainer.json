{
	"name": "app",

	"dockerComposeFile": "compose.yaml",
	"service": "dev",
	"workspaceFolder": "/workspace",

	"containerEnv": {
		"TZ": "${localEnv:TZ:Asia/Tokyo}"
	},

	"forwardPorts": ["httpd:8080"],

	"remoteUser": "vscode",

	"customizations": {
		"vscode": {
			"extensions": [
				"EditorConfig.EditorConfig",
				"ms-azuretools.vscode-docker"
			]
		}
	}
}
